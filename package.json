{
  "name": "org.javacream.training.typescript",
  "version": "1.0.0",
  "description": "Training project for TypeScript",
  "main": "index.js",
  "scripts": {
    "compile": "tsc --outDir ./dist -p .",
    "compile-watch": "tsc -w --outDir ./dist -p .",
    "serve": "nodemon ./dist/nodeserver.js",
    "start": "concurrently \"npm run compile-watch\"  \"npm run serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JavacreamTraining/org.javacream.training.node.typescript.git"
  },
  "keywords": [
    "TypeScript",
    "JavaScript",
    "Node"
  ],
  "author": "Rainer Sawitzki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JavacreamTraining/org.javacream.training.node.typescript/issues"
  },
  "homepage": "https://github.com/JavacreamTraining/org.javacream.training.node.typescript#readme",
  "devDependencies": {
    "@types/node": "^10.12.10",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "commonjs": "0.0.1"
  }
}
